{
  "version": "1.0",
  "defaultProvider": "unpkg",
  "libraries": [
    {
      "library": "vue@2.5.21",
      "destination": "wwwroot/lib/vue/",
      "files": ["dist/vue.min.js","dist/vue.js"]

    },
    {
      "library": "bootstrap-vue@2.0.0-rc.11",
      "destination": "wwwroot/lib/bootstrap-vue/"
    },
    {
      "library": "bootstrap@4.2.1",
      "destination": "wwwroot/lib/bootstrap/"
    },
    {
      "provider": "cdnjs",
      "library": "babel-polyfill@7.2.5",
      "destination": "wwwroot/lib/babel-polyfill"

    },
    {
      "provider": "cdnjs",
      "destination": "wwwroot/lib/turbolinks/",
      "library": "turbolinks@5.2.0"
    }
  ]
}